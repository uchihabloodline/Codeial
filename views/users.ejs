    <body>   
        <h1>
           <link rel="stylesheet" href="../assets/css/user_profile.css">
           Codeial/ <%= title %>
        </h1>

        <%if(user.id == profile_user.id){%> 

            <form action="/user/update/<%=profile_user.id %>" method="POST">
                <input type="text" name="name" placeholder="Enter Name" value="<%=profile_user.name%>" required>
                <input type="email" name="email" placeholder="Enter email" value="<%=profile_user.email%>" required>    
                <input type="submit" value="Update">
            </form>
        <%}else{%>
            <p>
                <%= profile_user.email %>   <br>
                <%= profile_user.name %>    <br>
            </p>
        <%}%>
        
    </body>
     